<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- tag(또는 node)의 순서에 주의 -->

	<!-- 전달인자로(insert/update null 허용) null 줄 수 있음 -->
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
		<!-- mapper에 사용할 타입이 길어서 짧게 별칭 지정 -->
	<typeAliases>
		<typeAlias type="project.dto.Bom_DTO" alias="Bom_DTO" />
		<typeAlias type="project.dto.Materials_DTO" alias="Materials_DTO" />
		<typeAlias type="project.dto.ProductionProcessDescription_DTO" alias="ProductionProcessDescription_DTO" />
		<typeAlias type="project.dto.Products_DTO" alias="Products_DTO" />
	</typeAliases>

    
</configuration>


