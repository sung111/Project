<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.emp">

	<select id="selectMaterials" resultType="project.dto.Materials_DTO">
		select * from MATERIALS  WHERE materialdel = 'n'
	</select>
	
	<select id="selectProducts" resultType="project.dto.Products_DTO">
		select * from products  WHERE productdel = 'n'
	</select>
	
</mapper>